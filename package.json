{
  "name": "template-nestjs-redis",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "concurrently -k -n APP,TS \"nodemon\" \"npm:tc:watch\"",
    "tc": "tsc -p tsconfig.json --noEmit",
    "tc:watch": "tsc -w -p tsconfig.json --noEmit",
    "build": "npm run build:swc",
    "start:prod": "node --enable-source-maps dist/main.js",
    "build:swc": "rimraf dist && swc ./src -d dist --strip-leading-paths --source-maps",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "test:verbose": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.910.0",
    "@aws-sdk/s3-presigned-post": "^3.910.0",
    "@aws-sdk/s3-request-presigner": "^3.910.0",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.17.1",
    "@sendgrid/mail": "^8.1.6",
    "@swc/register": "^0.1.10",
    "argon2": "^0.44.0",
    "aws-sdk-client-mock": "^4.1.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.61.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "file-type": "^21.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "nestjs-cls": "^6.0.1",
    "nestjs-pino": "^4.4.1",
    "nestjs-zod": "^5.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^10.0.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.2",
    "pino-std-serializers": "^7.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.4",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.13.5",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.10",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.[tj]sx?$": [
        "ts-jest",
        {
          "tsconfig": "./tsconfig.spec.json"
        }
      ]
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^file-type$": "<rootDir>/../test/mocks/file-type.ts",
      "^@aws-sdk/s3-request-presigner$": "<rootDir>/../test/mocks/s3-request-presigner.ts"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!uuid)/"
    ],
    "maxWorkers": "50%",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
